import init.macallan.usb.rc

on early-init
    mount debugfs debugfs /sys/kernel/debug

    # create directory for ASUS Product Demo
    mkdir /APD 0660 system system

    # persist
    mkdir /persist 0764 root system

on init

#    mount tmpfs tmpfs /Removable mode=0751,uid=0,gid=1028

    export SECONDARY_STORAGE /storage/MicroSD:/storage/SD:/storage/USBdisk1a:/storage/USBdisk1b:/storage/USBdisk1c:/storage/USBdisk1d:/storage/USBdisk1e:/storage/USBdisk1f:/storage/USBdisk1g:/storage/USBdisk1h:/storage/USBdisk2
    export TOUCH_CONF_DIR /persist/touch
    export TOUCH_DATA_DIR /persist/touch

    # Support legacy paths
    symlink /sdcard /storage/sdcard0


on fs
    setprop ro.crypto.tmpfs_options size=128m,mode=0771,uid=1000,gid=1000
    setprop ro.crypto.umount_sd false
    setprop ro.crypto.fuse_sdcard true
    mount_all /fstab.macallan

on post-fs-data
    mkdir /data/sensors 0701 root root
    mkdir /data/sensors/hw 0701  root root
    start halsel

    mkdir /data/misc/wifi 0770 wifi wifi
    mkdir /data/misc/wifi/sockets 0770 wifi wifi
    mkdir /data/misc/wifi/firmware 0770 wifi wifi
    mkdir /data/misc/dhcp 0770 dhcp dhcp
    mkdir /data/nvcam 0700 media camera
    chown dhcp dhcp /data/misc/dhcp
    chown wifi root /dev/rfkill

    mkdir /data/misc/wminput 0776 system system

    mkdir /data/media 0770 media_rw media_rw

    # Set indication (checked by vold) that we have finished this action
    setprop vold.post_fs_data_done 1

    # Create GPS folders and set its permissions
    mkdir /data/gps 775 system system
    chown system system /data/gps
    chmod 0777 /system/bin/gpsd

    # eMMC total size for AsusSetting
    symlink /sys/bus/mmc/devices/mmc0:0001/emmc_total_size /data/data/emmc_total_size

